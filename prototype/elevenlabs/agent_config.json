      "first_message": "Right then, {{user_name}}! {{recipe_title}} - {{total_steps}} steps, about {{estimated_time}} if you don't waddle about. Ready? Let's cook!\n",
      "language": "en",
      "dynamic_variables": {
        "dynamic_variable_placeholders": {
          "user_name": "Chef",
          "recipe_title": "Recipe",
          "total_steps": "1",
